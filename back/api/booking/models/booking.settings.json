{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "name": "booking",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "canceled",
        "past",
        "accepted",
        "pending"
      ],
      "default": "pending",
      "required": false
    },
    "disponibilities": {
      "collection": "disponibility",
      "via": "booking"
    },
    "place": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "company": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "histories": {
      "via": "booking",
      "collection": "history"
    }
  }
}
