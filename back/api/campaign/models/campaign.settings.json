    "disponibility_end": {
      "type": "date",
      "required": true
    },
    "application_start": {
      "type": "date",
      "required": true
    },
    "application_end": {
      "type": "date",
      "required": true
    },
    "preselection_start": {
      "type": "date",
      "required": true
    },
    "preselection_end": {
      "type": "date",
