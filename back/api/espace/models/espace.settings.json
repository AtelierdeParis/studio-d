{
  "kind": "collectionType",
  "collectionName": "place",
  "info": {
    "name": "Espace",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "surface": {
      "type": "decimal",
      "required": true
    },
    "roomLength": {
      "type": "decimal",
      "required": true
    },
    "width": {
      "type": "decimal",
      "required": true
    },
    "height": {
      "type": "decimal",
      "required": true
    },
    "mirror": {
      "type": "boolean",
      "required": true
    },
    "danceBar": {
      "type": "boolean",
      "required": true
    },
    "accomodation": {
      "type": "boolean",
      "required": true
    },
    "technicalStaff": {
      "type": "boolean",
      "required": true
    },
    "floor": {
      "type": "enumeration",
      "enum": [
        "plancherDanse",
        "parquetTraditionnel",
        "other",
        "todefine"
      ],
      "required": true
    },
    "otherFloor": {
      "type": "string"
    },
    "about": {
      "type": "text",
      "required": false
    },
    "details": {
      "type": "text"
    },
    "address": {
      "type": "string",
      "required": true
    },
    "files": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "espaces"
    },
    "disponibilities": {
      "via": "espace",
      "collection": "disponibility"
    },
    "scheduleDetails": {
      "type": "text"
    },
    "filledUntil": {
      "type": "date"
    },
    "published": {
      "type": "boolean",
      "default": true
    },
    "bookings": {
      "via": "espace",
      "collection": "booking"
    },
    "country": {
      "type": "string",
      "required": true
    },
    "external_id": {
      "type": "integer"
    },
    "danceCarpet": {
      "type": "enumeration",
      "enum": [
        "true",
        "false",
        "possible"
      ],
      "required": false
    },
    "slug": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "city": {
      "via": "espaces",
      "model": "city"
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "longitude": {
      "type": "float"
    },
    "deleted": {
      "type": "boolean",
      "default": false
    },
    "applications": {
      "via": "espace",
      "collection": "application"
    },
    "campaign_files": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
